      <h1>
        Información distrito
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-map-marker"></i> Distrito</a></li>
        <li class="active">Ver información de distrito</li>
      </ol>
    </section>
{% endblock %}
{% block content -%}
